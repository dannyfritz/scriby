{
  "name": "scriby",
  "version": "0.0.1",
  "description": "Scriby Generates Bare-Bones Single Page Documentation with YUIDoc (JSDoc) Comments.",
  "license": "MIT",
  "author": "Danny Fritz",
  "people": [
    {
      "name": "Danny Fritz",
      "email": "dannyfritz@gmail.com",
      "url": "http://dannyfritz.com/"
    }
  ],
  "scripts": {
    "test": "npm run lint && npm run unittest && npm run coverage",
    "build": "npm run docs",
    "lint": "eslint lib test",
    "unittest": "tape test/*.js | tap-spec",
    "coverage": "browserify -t coverify test/scriby-test.js --bare | node | coverify --quiet",
    "docs": "node bin/cli.js lib"
  },
  "bin": "./lib/cli.js",
  "homepage": "http://github.com/dannyfritz/scriby.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/dannyfritz/scriby"
  },
  "keywords": [
    "documentation",
    "docs",
    "apidocs",
    "jsdoc",
    "javadoc",
    "yuidoc"
  ],
  "bugs": {
    "url": "https://github.com/dannyfritz/scriby/issues",
    "email": "dannyfritz@gmail.com"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "./lib",
    "./bin"
  ],
  "directories": {
    "lib": "lib",
    "bin": "bin",
    "doc": "docs"
  },
  "dependencies": {
    "optimist": "^0.6.1",
    "lodash": "^2.4.1",
    "handlebars": "^2.0.0-alpha.2",
    "yuidocjs": "^0.3.49",
    "marked": "^0.3.2",
    "file": "^0.2.2",
    "node-serialize": "0.0.3"
  },
  "devDependencies": {
    "browserify": "^5.10.1",
    "coverify": "^1.0.7",
    "eslint": "^0.7.4",
    "tap-spec": "^0.2.1",
    "tape": "^2.14.0"
  }
}
